<?xml version="1.0" encoding="utf-8"?><extension type="module" version="3.9" client="site" method="upgrade">	<name>mod_metatags</name>	<author>Alexon Balangue</author>	<creationDate>19 april 2019</creationDate>	<copyright>Copyright (C) 2012-2020. All rights reserved.</copyright>	<license>http://www.gnu.org/licenses/gpl-3.0.html GNU/GPL</license>	<authorEmail>contact@alexonbstudio.fr</authorEmail>	<authorUrl>www.alexonbalangue.me</authorUrl>	<version>4.3</version>	<description>MOD_METATAGS_XML_DESCRIPTION</description>	<files>		<filename module="mod_metatags">mod_metatags.php</filename>		<filename>mod_metatags.xml</filename>		<folder>tmpl</folder>		<folder>language</folder>		<filename>index.html</filename>		</files>	<languages folder="language">		<language tag="en-GB">en-GB/en-GB.mod_metatags.ini</language>		<language tag="fr-FR">fr-FR/fr-FR.mod_metatags.ini</language>		<language tag="en-GB">en-GB/en-GB.mod_metatags.sys.ini</language>		<language tag="fr-FR">fr-FR/fr-FR.mod_metatags.sys.ini</language>	</languages>		<config>		<fields name="params">			<fieldset name="basic" label="Basic">				<field 					name="robots"					type="list"					default="index, follow"					label="robots"					description="">						<option value="">JFIELD_MOD_METATAGS_CLASSIFICATION_OPTION_0</option>						<option value="index, follow">index, follow</option>						<option value="noindex, follow">noindex, follow</option>						<option value="index, nofollow">index, nofollow</option>						<option value="noindex, nofollow">noindex, nofollow</option>				</field>				<field 					type="media"					name="logo"					default=""					label="Logo"					description="" 					directory="" />			</fieldset>			<fieldset name="twitter" label="Twitter Cards">				<field 					name="twitter-card"					type="list"					default="summary"					label="Twitter Card"					description="" >						<option value="">JFIELD_MOD_METATAGS_CLASSIFICATION_OPTION_0</option>						<option value="summary">Summary</option>						<option value="summary_large_image">Summary Large image</option>				</field>								<field 					type="text"					name="twitter-user"					label="username"					default="alexonbstudio"					description="" />										</fieldset>			<fieldset name="open-graph" label="Open Graph">				<field 					name="ogtype"					type="list"					default="article"					label="JFIELD_MOD_METATAGS_OGTYPE_LABEL"					description="JFIELD_MOD_METATAGS_OGTYPE_DESC">						<option value="">JFIELD_MOD_METATAGS_CLASSIFICATION_OPTION_0</option>						<option value="website">website</option>						<option value="article">article</option>				</field>				<field 					name="fb-admins"					type="text"					default=""					label="JFIELD_MOD_METATAGS_FBADMIN_LABEL"					description="JFIELD_MOD_METATAGS_FBADMIN_DESC" />				<field 					name="fb-app-id"					type="text"					default=""					label="JFIELD_MOD_METATAGS_FBAPP_LABEL"					description="JFIELD_MOD_METATAGS_FBAPP_DESC" />				<field 					name="fb-profils_id"					type="text"					default=""					label="MOD_METATAGS_FBPROFILSID_LABEL"					description="MOD_METATAGS_FBPROFILSID_DESC" />			</fieldset>						<fieldset name="mod_meta-Shareaholic" label="Shareaholic">				<field 					name="shareaholic-site-id-api"					type="text"					default=""					label="Shareaholic API/Site ID"					description="info http://shareaholic.com" />				</fieldset>					<fieldset name="mod_meta-backlinks" label="script &amp; Backlinks">				<field name="head-script-frontend" type="textarea" default="" label="Enter some html (HEAD)" description="" rows="10" cols="5" filter="raw"/>				<field name="footer-script-frontend" type="textarea" default="" label="Enter some html (BODY)" description="" rows="10" cols="5" filter="raw"/>						</fieldset>			<fieldset name="mod_meta-json-ld" label="JSON-LD ">					<field 					name="json-ld-TYPE"					type="list"					default="json-ld-person"					label="JFIELD_MOD_METATAGS_OGTYPE_LABEL"					description="JFIELD_MOD_METATAGS_OGTYPE_DESC">						<option value="">JFIELD_MOD_METATAGS_CLASSIFICATION_OPTION_0</option>						<option value="json-ld-person">Schema.org -¨Person</option>						<option value="json-ld-organisation">Schema.org -¨Organisation</option>						<option value="json-ld-custom">Custom</option>				</field>			</fieldset>						<fieldset name="mod_meta-business" label="Business">								<field 					name="gtagsmanager"					type="text"					default=""					label="Google Tags Manager"					description="Code GTM" />			</fieldset>			<fieldset name="analystics" label="Analystic">						<field 					name="analyse-UA"					type="text"					default=""					label="Analystic UA"					description="JFIELD_MOD_METATAGS_DOMAINESNAME_DESC" />				</fieldset>		</fields>	</config></extension>